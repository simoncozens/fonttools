@uc1 = [A B C D E];
@uc2 = [F G H I J K L];
@lc1 = [a b c d e f g h];
@lc2 = [i j k l m n o p];
@lc3 = [q r s t u v w x];

lookup lookup1 {
    sub a by a;
} lookup1;

lookup lookup2 {
    sub b by b;
} lookup2;

lookup lookup3 {
    sub c by c;
} lookup3;

feature rlig {
    sub @uc1' lookup lookup1 @lc1 @lc1;
    sub @uc1' lookup lookup2 @lc1 @lc2;
    sub @uc1' lookup lookup3 @lc1 @lc3;
    sub @uc1' lookup lookup2 @lc2 @lc1;
    sub @uc1' lookup lookup3 @lc2 @lc2;
    sub @uc1' lookup lookup1 @lc2 @lc3;
 
    sub @uc2' lookup lookup3 @lc1 @lc1;
    sub @uc2' lookup lookup1 @lc1 @lc2;
    sub @uc2' lookup lookup2 @lc1 @lc3;
    sub @uc2' lookup lookup3 @lc2 @lc1;
    sub @uc2' lookup lookup1 @lc2 @lc2;
    sub @uc2' lookup lookup2 @lc2 @lc3;
} rlig;